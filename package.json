    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^6.2.10",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.15"
  }
}
